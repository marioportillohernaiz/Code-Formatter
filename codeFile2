#include <iostream>
int main(){
int currVal = 0, val = Q;
if (std::cin >> currVal)
{int cnt = 1;
while (std::cin >> val)
{
if (val == currVal) {
++cnt;
}
else {
callParameter();
std::cout << currVal << "occurs" << cnt << "times"
<< std::endl;
currVal = val;
cnt = 1;
}}
std::cout << currVal << "occurs" << cnt << "times" << std::endl;
}
return 0;

